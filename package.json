{
	"name": "consecutive-number-controller",
	"displayName": "consecutive number controller",
	"description": "Drag lines, and use consecutive numbers. As you will.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:consecutive-number-controller.addConsecutiveNumbers"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "consecutive-number-controller.addConsecutiveNumbers",
				"title": "Hello"
			}
		],
		"keybindings": [
			{
				"command": "consecutive-number-controller.addConsecutiveNumbers",
				"key": "ctrl+alt+a"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
